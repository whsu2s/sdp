<?xml version="1.0"?>
<launch>

  <!-- the actual move base node -->
  <node pkg="move_base" type="move_base" name="move_base" respawn="false" output="screen">
    <rosparam file="$(find g3_move_base)/config/costmap_common.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find g3_move_base)/config/costmap_common.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find g3_move_base)/config/costmap_local.yaml" command="load" ns="local_costmap" />
    <rosparam file="$(find g3_move_base)/config/costmap_global.yaml" command="load" ns="global_costmap" />
    <rosparam file="$(find g3_move_base)/config/base_local_planner.yaml" command="load" />

  </node>


</launch>
